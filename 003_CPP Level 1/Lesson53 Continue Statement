using System;
using System.Collections.Generic;
using System.Linq;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            int result = 0;
            for (int i = 0; i < 5; i++)
            {
                Console.Write("Please Enter the number:");
                int number = int.Parse(Console.ReadLine());

                if (number > 50)
                {
                    Console.WriteLine("The number is greater than 50 and wan't be calclated");
                    continue;
                }
                result += number;
            }
            Console.WriteLine(result);

        }

    }
}
