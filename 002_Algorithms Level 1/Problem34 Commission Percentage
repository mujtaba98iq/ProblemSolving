solving problem 33:
#include <iostream>
using namespace std;


int ReadDegree(int From,int To) {
	int Grade;
	do {
	cout << "Enter the Grade between 0 and 100? : \n";
	cin >> Grade;
	} while (Grade <From || Grade >To );

	return Grade;

}

char CheckDegree(int Grade) {
	if (Grade >= 90)
		return 'A';
	else if (Grade >= 80 && Grade <= 89)
		return 'B';

	else if (Grade >= 70 && Grade <= 79)
		return 'C';

	else if (Grade >= 60 && Grade <= 69)
		return 'D';

	else if (Grade >= 50 && Grade <= 59)
		return 'E';

	else
		return 'F';

}





int main()
{
	cout<<endl<<"THe Grade is "<<CheckDegree(ReadDegree(0, 100));

	return 0;

}
